{
    "name": "ufo-tech/json-rpc-client-sdk",
    "description": "Simple clientSDK builder for any json-RPC servers",
    "version": "4.0.1",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Maystrenko",
            "email": "ashterix69@gmail.com"
        }
    ],
    "type" : "library",
    "homepage": "https://docs.ufo-tech.space/bin/view/docs/JsonRpcClientSdk/?language=en",
    "keywords" : ["rpc", "async", "async api", "async rpc", "async transport", "ufo", "api", "sdk", "sdk generator", "rpc client", "json-rpc"],
    "require" : {
        "php": ">=8.3",
        "symfony/filesystem": "^7.2",
        "symfony/cache": "^7.2",
        "symfony/maker-bundle": "^1.48",
        "symfony/http-client": "^7.2",
        "ufo-tech/rpc-objects": "^3.3",
        "ufo-tech/dto-transformer": "^2",
        "ufo-tech/rpc-exceptions": "^1",
        "ext-readline": "*",
        "ufo-cms/colored_cli": "^1.0",
        "symfony/validator": "^7.2",
        "symfony/messenger": "^7.2",
        "symfony/amqp-messenger": "^7.2",
        "symfony/serializer": "^7.2",
        "symfony/yaml": "^7.1",
        "ext-intl": "*"
    },
    "require-dev" : {
        "phpunit/phpunit": "^10.0",
        "symfony/test-pack": "^1.0"
    },
    "autoload" : {
        "psr-4" : {
            "Ufo\\RpcSdk\\": "src/",
            "Ufo\\RpcSdk\\Client\\" : "sdk/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ufo\\RpcSdk\\Tests\\": "tests/"
        }
    },
    "extra" : {
    },
    "scripts": {
        "auto-scripts": {
        },
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    }
}
