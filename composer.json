{
    "name": "ufo-tech/json-rpc-client-sdk",
    "description": "Simple clientSDK builder for any json-RPC servers",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Maystrenko",
            "email": "ashterix69@gmail.com"
        }
    ],
    "type" : "library",
    "homepage": "https://github.com/ufo-tech/json-rpc-client-sdk",
    "keywords" : ["ufo", "api", "sdk", "sdk generator", "rpc client", "json-rpc"],
    "require" : {
        "php": ">=8.1",
        "symfony/filesystem": "6.*",
        "symfony/cache": "6.*",
        "symfony/maker-bundle": "^1.48",
        "symfony/http-client": "6.*",
        "ufo-tech/rpc-objects": "*",
        "ufo-tech/rpc-exceptions": "*",
        "ext-readline": "*",
        "ufo-cms/colored_cli": "^1.0"
    },
    "autoload" : {
        "psr-4" : {
            "Ufo\\RpcSdk\\": "src/",
            "Ufo\\RpcSdk\\Client\\" : "sdk/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ufo\\RpcSdk\\Tests\\": "tests/"
        }
    },
    "extra" : {
    },
    "scripts": {
        "auto-scripts": {
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    }
}
